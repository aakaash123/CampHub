        {
          z-index:30;
           position:absolute;
         bottom:1000%;
          left:20%;
        width:5px;
        	height: 10px;
          border-radius:25%;
          background:#994d00;
          opacity:0.6;
           animation: bounce 2.5s infinite linear;
        }
        .smoke1
        {
          z-index:30;
           position:absolute;
          bottom:800%;
          left:17%;
        width:5px;
        	height: 10px;
          border-radius:25%;
          background:#994d00;

           animation: bounce 2.5s infinite linear;
        }
        .smoke2
        {
          z-index:30;
           position:absolute;
          bottom:1100%;
          left:17%;
        width:5px;
        	height: 10px;
          border-radius:25%;
          background:#994d00;
          opacity:0.6;
          animation: bounce 2.5s infinite linear;
        }
        .smoke3
        {
          z-index:30;
           position:absolute;
         bottom:1300%;
          left:20%;
        width:5px;
        	height: 5px;
          border-radius:25%;
          background:#994d00;
          opacity:0.48;
          animation: bounce 2.5s infinite;
        }
        .smoke4
        {
          z-index:30;
           position:absolute;
           bottom:1400%;
          left:17%;
        width:5px;
        	height: 5px;
          border-radius:25%;
          background:#994d00;
          opacity:0.44;
          animation: bounce 3.5s infinite linear;
        }
        @-webkit-keyframes "bounce" {
        	0% {
        		-webkit-transform:  translateY(0px);
        		transform:  translateY(0px);
        	}
        	100% {
        		-webkit-transform:  translateY(-10px);
        		transform:  translateY(-10px);
        	}
        	100% {
        	opacity:0;
        	}
        }
        .cloud
        {
          z-index:100;
          position:absolute;
         bottom:1500%;
          left:57%;
          width:40px;
          height:10px;
          background:#fff;
          border-radius:15px;
          opacity:0.7;
           animation: float 19.0s infinite linear;
        }
        .cloud1
        {
          z-index:100;
          position:absolute;
          bottom:1400%;
          left:51%;
          width:40px;
          height:10px;
          background:#fff;
          border-radius:15px;
          opacity:0.7;
           animation: float 19.5s infinite linear;
        }
        .cloud2
        {
          z-index:100;
          position:absolute;
          bottom:1500%;
          left:77%;
          width:40px;
          height:10px;
          background:#fff;
          border-radius:15px;
          opacity:0.7;
           animation: float 17.5s infinite linear;
        }
        .cloud3
        {
          z-index:100;
